{
    "name": "khody2012/laravel-ami-toolkit",
    "description": "A powerful and developer-friendly Laravel package that provides seamless integration with Asterisk AMI (Asterisk Manager Interface).",
    "keywords": [
        "laravel",
        "asterisk",
        "ami",
        "voip",
        "pbx",
        "telephony",
        "event-driven",
        "call center",
        "realtime"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/events": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Khody2012\\LaravelAmiToolkit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Khody",
            "email": "khodarahmpoor@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khody2012\\LaravelAmiToolkit\\LaravelAmiServiceProvider"
            ]
        }
    }
}
